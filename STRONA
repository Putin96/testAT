.ocena {
  border: 2px solid #ccc;
  background-color: #eee;
  border-radius: 5px;
  padding: 16px;
  margin: 16px 0;
}

/* Clear floats after ocenas */
.ocena::after {
  content: "";
  clear: both;
  display: table;
}

/* Float images inside the ocena to the left. Add a right margin, and style the image as a circle */
.ocena img {
  float: left;
  margin-right: 20px;
  border-radius: 50%;
}

/* Increase the font-size of a span element */
.ocena span {
  font-size: 20px;
  margin-right: 15px;
}

/* Add media queries for responsiveness. This will center both the text and the image inside the ocena */
@media (max-width: 500px) {
  .ocena {
    text-align: center;
  }

  .ocena img {
    margin: auto;
    float: none;
    display: block;
  }
}
